<UserControl x:Class="Lunalipse.Presentation.LpsComponent.LpsProgressBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Lunalipse.Presentation.LpsComponent"
             mc:Ignorable="d" Height="20" MaxHeight="20"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="auto" MaxHeight="20">
        <Border CornerRadius="10" x:Name="Track">
            <Grid Height="auto" x:Name="ProgressBar" HorizontalAlignment="Left">
                <Border CornerRadius="10" x:Name="ProgressBarBackground">
                </Border>
            </Grid>
        </Border>
    </Grid>
</UserControl>
