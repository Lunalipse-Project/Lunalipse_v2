<base1:LunalipseDialogue x:Class="Lunalipse.Windows.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:base1="clr-namespace:Lunalipse.Presentation.LpsWindow;assembly=Lunalipse.Presentation"
        xmlns:LpsComponentBase="clr-namespace:Lunalipse.Presentation.LpsComponent.Base;assembly=Lunalipse.Presentation"
        xmlns:LpsComponent="clr-namespace:Lunalipse.Presentation.LpsComponent;assembly=Lunalipse.Presentation"
        xmlns:local="clr-namespace:Lunalipse.Windows"
        mc:Ignorable="d" Closing="LunalipseDialogue_Closing"
        Title="CORE_SETTING" Height="530" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".25*"/>
            <ColumnDefinition Width=".75*"/>
        </Grid.ColumnDefinitions>
        <LpsComponentBase:LpsDetailedListView Grid.Column="0" Name="SPlanelSlider" ListIconSize="{StaticResource IconLarge}" FontSize="{StaticResource FontNormal}" Background="Transparent"/>
        <ScrollViewer Grid.Column="1" Margin="5,0,0,0" x:Name="Frame_Scrollviewer">
            <LpsComponent:FramePresentor x:Name="SPanleViewer"></LpsComponent:FramePresentor>
        </ScrollViewer>
    </Grid>
</base1:LunalipseDialogue>
